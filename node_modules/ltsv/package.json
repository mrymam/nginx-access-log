{
  "name": "ltsv",
  "description": "LTSV parser, formatter, validator and TransformStream",
  "version": "2.0.0",
  "author": "sasa+1 <sasaplus1@gmail.com>",
  "browser": "./dist/umd/ltsv.js",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.7.3",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "espower-typescript": "^9.0.2",
    "husky": "4.2.5",
    "karma": "5.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-mocha": "2.0.1",
    "karma-rollup-preprocessor": "7.0.5",
    "lint-staged": "10.2.11",
    "mocha": "8.0.1",
    "npm-run-all": "^4.1.5",
    "power-assert": "1.6.1",
    "prettier": "2.0.5",
    "rollup": "2.22.2",
    "rollup-plugin-terser": "6.1.0",
    "rollup-plugin-typescript": "^1.0.1",
    "typedoc": "^0.17.0",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/sasaplus1/ltsv.js.git"
  },
  "scripts": {
    "build": "run-p -l build:*",
    "build-documents": "typedoc ./src",
    "build:cjs": "tsc",
    "build:esm": "rollup -c --environment build:esm",
    "build:umd": "rollup -c --environment build:umd",
    "check-types": "tsc --noEmit",
    "fix": "eslint --fix --ext .js,.ts .",
    "fixpack": "npx fixpack",
    "lint": "eslint --cache --ext .js,.ts .",
    "pretest": "run-s build:cjs",
    "pretest:browser": "run-s build:umd",
    "test": "mocha",
    "test:browser": "karma start --single-run"
  },
  "sideEffects": false,
  "types": "./dist/cjs/index.d.ts"
}
